{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es2020": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "indent": ["error", 2]
  },
  "plugins": ["prettier"]
}
